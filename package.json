{
  "name": "vuetify-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepreview": "npm run build && npm run copyItems && npm run copyVillagers",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "copyItems": "node -e \"require('fs').cpSync('./src/assets/Items', './dist/assets/Items', {recursive: true});\"",
    "copyVillagers": "node -e \"require('fs').cpSync('./src/assets/Villagers', './dist/assets/Villagers', {recursive: true});\"",
    "predeploy": "npm run build && npm run copyItems && npm run copyVillagers",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@chenfengyuan/vue-countdown": "^2.1.2",
    "@mdi/font": "7.0.96",
    "@vueuse/core": "^10.6.0",
    "core-js": "^3.29.0",
    "roboto-fontface": "*",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.37.0",
    "eslint-plugin-vue": "^9.3.0",
    "gh-pages": "^6.0.0",
    "sass": "^1.60.0",
    "unplugin-fonts": "^1.0.3",
    "vite": "^4.2.0",
    "vite-plugin-vuetify": "^1.0.0"
  }
}
